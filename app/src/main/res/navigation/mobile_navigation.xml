<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.zzs.ppjoke_kotlin_jetpack.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.zzs.ppjoke_kotlin_jetpack.ui.sofa.SofaFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_sofa" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.zzs.ppjoke_kotlin_jetpack.ui.my.MyFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
</navigation>